import { OnInit, OnDestroy } from '@angular/core';
import Store from 'devextreme/data/abstract_store';
import { Options as DataSourceOptions } from 'devextreme/data/data_source';
import { NestedOptionHost } from 'devextreme-angular/core';
import { NestedOption } from 'devextreme-angular/core';
export declare class DxoLookupComponent extends NestedOption implements OnDestroy, OnInit {
    allowClearing: boolean;
    calculateCellValue: Function;
    dataSource: DataSourceOptions | Store | Function | Array<any>;
    displayExpr: Function | string;
    valueExpr: string | Function;
    protected readonly _optionPath: string;
    constructor(parentOptionHost: NestedOptionHost, optionHost: NestedOptionHost);
    ngOnInit(): void;
    ngOnDestroy(): void;
}
export declare class DxoLookupModule {
}
